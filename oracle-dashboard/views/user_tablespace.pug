table.w3-table.w3-border.w3-bordered.w3-hoverable
    tr
        th Tablespace 
        th Quota
        th Used (Mb)
        th
    for entrada in ut
        th= entrada.tablespace_name 
        if (entrada.quota < 0)
            th Unlimited
        else
            th= entrada.quota
        th= entrada.used_quota 
        td
            a(href="/tablespaces/" + entrada.tablespace_id)
                i.fas.fa-eye
